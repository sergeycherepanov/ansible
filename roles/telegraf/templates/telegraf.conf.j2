[global_tags]
[agent]
  interval = "5s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "5s"
  flush_interval = "10s"
  flush_jitter = "10s"
  precision = ""
  debug = false
  quiet = false
  logfile = ""
  hostname = "vidi-alle.com.ua"
  omit_hostname = false
[[outputs.influxdb]]
  urls = ["{{ influxdb_url }}"] # required
  database = "nextweb" # required
  retention_policy = ""
  write_consistency = "any"
  timeout = "5s"
  username = "{{ influxdb_user }}"
  password = "{{ influxdb_password }}"
[[inputs.cpu]]
  percpu = true
  totalcpu = true
  collect_cpu_time = false
[[inputs.disk]]
  ignore_fs = ["tmpfs", "devtmpfs"]
[[inputs.kernel]]
[[inputs.mem]]
[[inputs.processes]]
[[inputs.swap]]
[[inputs.system]]
[[inputs.net]]
